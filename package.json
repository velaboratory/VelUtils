{
  "name": "com.franzco.unityutilities",
  "displayName": "unityutilities",
  "version": "2.13.16",
  "unity": "2019.1",
  "description": "A lightweight collection of flexible utilities for VR projects.",
  "keywords": [
    "virtual reality",
    "boilerplate",
    "utilities"
  ],
  "author": {
    "name": "Anton Franzluebbers",
    "email": "anton@uga.edu",
    "url": "https://cobweb.cs.uga.edu/~anton"
  },
  "samples": [
    {
      "displayName": "Oculus SDK Support",
      "description": "Includes support for Oculus SDK features. Main feature is faster input with OVRInput.",
      "path": "Samples~/OculusSDKSupport"
    },
    {
      "displayName": "Finger Touch Module (Oculus)",
      "description": "Module to use an Oculus Avatar finger or Quest hand tracking finger as a touch input for UI. Requires Oculus Integration.",
      "path": "Samples~/FingerTouchModule"
    },
    {
      "displayName": "XR Interaction",
      "description": "Includes XR Interaction compatible scripts. Requires the XR Interaction Toolkit package",
      "path": "Samples~/XRInteractionModule"
    },
    {
      "displayName": "Pico Neo 2 Support",
      "description": "Includes an input module for the Pico Neo 2 controllers",
      "path": "Samples~/Pico Neo 2 Support"
    },
    {
      "displayName": "XR Interaction V2",
      "description": "Compatible with XRInteractionToolkit v1.0.0",
      "path": "Samples~/XRInteractionModule_v2"
    },
    {
      "displayName": "InputSystem Support",
      "description": "Compatible with the new InputSystem",
      "path": "Samples~/InputSystem Support"
    }
  ],
  "dependencies": {
    "com.unity.nuget.newtonsoft-json": "2.0.0-preview",
    "com.unity.xr.legacyinputhelpers": "2.1.7"
  }
}
